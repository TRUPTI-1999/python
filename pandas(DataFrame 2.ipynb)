   "cell_type": "markdown",
   "id": "b6b89394",
   "metadata": {},
   "source": [
    "### Suffix"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 131,
   "id": "bcc3230b",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/html": [
       "<div>\n",
       "<style scoped>\n",
       "    .dataframe tbody tr th:only-of-type {\n",
       "        vertical-align: middle;\n",
       "    }\n",
       "\n",
       "    .dataframe tbody tr th {\n",
       "        vertical-align: top;\n",
       "    }\n",
       "\n",
